{
  "extends": "stylelint-config-standard",
  "rules": {
    "at-rule-no-unknown": [true, {"ignoreAtRules": ["mixin"]}],
    "selector-list-comma-newline-after": null,
    "function-name-case": null,
    "property-no-unknown": [
      true,
      {
        "ignoreProperties": [
          "composes",
          "compose-with"
        ]
      }
    ]
  }
}
